<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Guia perguntas</title>
  </head>
  <body>
    <h1>Guia Perguntas</h1>

    <hr />

    <ul>
      <li>Nome: <%= name%></li>
      <li>Idoma: <%= lang%></li>
      <li>Empresa: <%= company%></li>
    </ul>

    <% if(showMessage){ %>
    <p>Deu certo</p>
    <% }else{ %>
    <p>Isso Ã© uma mensagem de erro</p>
    <% } %>

    <table>
      <thead>
        <tr>
          <th>Nome</th>
          <th>Idade</th>
        </tr>
      </thead>
      <tbody>
        <% clients.forEach((client) => { %>
        <tr>
          <td><%=client.name %></td>
          <td><%=client.age %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
